{
  "name": "estudia/inertia-uno",
  "description": "A Laravel package that provides easy integration of the Inertia.js with the power of Unocss and auto import",
  "type": "library",
  "extra": {
    "laravel": {
      "providers": [
        "Estudia\\InertiaUno\\InertiaUnoServiceProvider"
      ],
      "commands": [
        "Estudia\\InertiaUno\\Console\\Commands\\InstallUno"
      ]
    }
  },
  "require": {
    "php": "^7.3 || ^8.2",
    "illuminate/console": "^8.0 || ^9.0 || ^10.10",
    "js-phpize/js-phpize": "^2.0 || ^2.9"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Estudia\\InertiaUno\\": "src/"
    }
  },
  "authors": [
    {
      "name": "ali Joder",
      "email": "ali.masoudi.alavi@gmail.com"
    }
  ]
}
